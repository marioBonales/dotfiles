#git aliases
alias g='git status'
alias gs='git status'
alias gb='git branch'
alias ga='git add .'
alias gc='git checkout'
alias gd='git branch -D'
alias gp='git pull'
alias pull='git pull'
alias push='git push'
alias gpsh='git push'
alias ack='ack-grep'
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias l='ls'

function gvim () { /usr/bin/gvim -f $* & }
function mvim () { /usr/bin/gvim -f $
function dif(){
  vimdiff <(git show HEAD~1:$1) $1
}
